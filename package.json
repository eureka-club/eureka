{
  "name": "eureka",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev1": "NODE_OPTIONS='--inspect' NODE_ENV=development TS_NODE_COMPILER_OPTIONS='{ \"module\":\"commonjs\" }' ts-node server.ts",
    "dev": "NODE_OPTIONS='--inspect' NODE_ENV=development next dev",
    "knex": "TS_NODE_COMPILER_OPTIONS='{ \"module\":\"commonjs\" }' knex",
    "lint": "eslint '**/*.{ts,tsx}'",
    "lt": "yarn lint && yarn tsc",
    "prisma": "prisma",
    "prisma-pf": "prisma --preview-feature",
    "start1": "NODE_ENV=production TS_NODE_COMPILER_OPTIONS='{ \"module\":\"commonjs\" }' ts-node server.ts",
    "start": "NODE_ENV=production next start",
    "tsc": "tsc --noEmit",
    "e2e": "cypress run",
    "e2e:record": "cypress run --record",
    "test:ci": "npm-run-all -p --race start e2e",
    "test": "npm-run-all -p --race start cy:open",
    "test:ci:record": "npm-run-all -p --race start e2e:record",
    "cy:verify": "cypress verify",
    "cy:info": "cypress info",
    "cy:run": "cypress run",
    "cy:run:record": "cypress run --record",
    "cy:open": "cypress open",
    "prepare": "husky install"
  },
